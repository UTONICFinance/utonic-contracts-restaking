{
  "name": "utonic-contract",
  "version": "0.0.1",
  "scripts": {
    "compile-operator-strategy-share": "npx func-js contracts/strategy/operator_strategy_share/operator_strategy_share.func --boc build/operator_strategy_share.cell",
    "compile-strategy-jetton": "npx func-js contracts/strategy/strategy_jetton/strategy_jetton.func --boc build/strategy_jetton.cell",
    "compile-strategy-ton": "npx func-js contracts/strategy/strategy_ton/strategy_ton.func --boc build/strategy_ton.cell",
    "compile-user-strategy-info": "npx func-js contracts/strategy/user_strategy_info/user_strategy_info.func --boc build/user_strategy_info.cell",
    "compile-strategy-withdraw": "npx func-js contracts/strategy/withdraw/strategy_withdraw.func --boc build/strategy_withdraw.cell",
    "compile-operator-register": "npx func-js contracts/utonic_manager/operator_register/operator_register.func --boc build/operator_register.cell",
    "compile-utonic-manager": "npx func-js contracts/utonic_manager/utonic_manager.func --boc build/utonic_manager.cell",

    "compile-test-jetton-minter": "npx func-js contracts/test/jetton/minter.func --boc build/test_jetton_minter.cell",
    "compile-test-jetton-wallet": "npx func-js contracts/test/jetton/wallet.func --boc build/test_jetton_wallet.cell",
    "compile-stake-test-update-code": "npx func-js contracts/test/test_update_code.func --boc build/stake_test_update_code.cell",

    "test": "jest --verbose"
  },
  "devDependencies": {
    "@ton/blueprint": "^0.22.0",
    "@ton/core": "^0.57.0",
    "@ton/crypto": "^3.2.0",
    "@ton/sandbox": "^0.20.0",
    "@ton/test-utils": "^0.4.2",
    "@ton/ton": "^13.11.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "ini": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
